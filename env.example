# Docker Compose Configuration
COMPOSE_PROJECT_NAME=qversity

# Airflow Configuration
AIRFLOW_UID=50000
AIRFLOW_PROJ_DIR=.
_AIRFLOW_WWW_USER_USERNAME=admin
_AIRFLOW_WWW_USER_PASSWORD=admin

# PostgreSQL Configuration
POSTGRES_USER=qversity-admin
POSTGRES_PASSWORD=qversity-admin
POSTGRES_DB=qversity

# dbt Configuration
DBT_PROFILES_DIR=/dbt
DBT_PROJECT_DIR=/dbt

# Bronze Layer Configuration
BRONZE_S3_BUCKET=qversity-raw-public-data
BRONZE_S3_KEY=mobile_customers_messy_dataset.json
BRONZE_POSTGRES_CONN=qversity_postgres